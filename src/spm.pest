WHITESPACE = _{ " " | "\t" | NEWLINE }

constant = @{ "⊥" }
op = @{ "?" | "&" | "∧" | "|" | "∨" | "->" | "→" }
var = @{ ASCII_ALPHA_UPPER ~ ASCII_ALPHANUMERIC* }
word = @{ ASCII_ALPHA_LOWER ~ (ASCII_ALPHANUMERIC | "_")* }

quote = { "[" ~ expr* ~ "]" }
expr = _{ constant | op | var | word | quote }
program = _{ SOI ~ expr* ~ EOI }
